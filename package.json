    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.1.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.10",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "execa": "^9.5.2",
    "fs-extra": "^11.3.0",
    "prettier": "3.4.2",
    "sass": "^1.83.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "unocss": "^65.4.3",
    "vite": "^6.0.11",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0"
